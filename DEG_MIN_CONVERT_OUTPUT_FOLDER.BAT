>"TEMP_COORD_FILE_WILL_DELETE.txt" ECHO N0%COORD_ENTRY%

powershell -Command "(gc TEMP_COORD_FILE_WILL_DELETE.txt) -replace '""N/', '.000 W' | Out-File -Encoding ASCII TEMP_COORD_FILE_WILL_DELETE.txt"

